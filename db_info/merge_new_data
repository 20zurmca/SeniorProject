-- Union the new and old roster master data

ALTER TABLE roster_master_data DROP COLUMN id;
ALTER TABLE high_school_data DROP COLUMN id;

CREATE TABLE roster_master_data3 AS (
	SELECT *
	FROM roster_master_data
	UNION
	SELECT *
	FROM roster_master_data2
);

DROP TABLE roster_master_data;
DROP TABLE roster_master_data2;

ALTER TABLE roster_master_data3 RENAME TO roster_master_data;
